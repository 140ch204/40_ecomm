<div class="container-fluid">
    <div class="hedder-up row">
        <div class="col-lg-3 col-md-3 logo-head">
            
                <%= link_to root_path do %> 
                <%= image_tag "Nekopix.png", class: "navbar-brand" %>
                <% end %>
            
        </div>
        <div class="col-lg-5 col-md-6 search-right">
            <form class="form-inline my-lg-0">
                <input class="form-control mr-sm-2" type="search" placeholder="Search">
                <button class="btn" type="submit">Search</button>
            </form>
        </div>
        <div class="col-lg-4 col-md-3 right-side-cart content-center">
            <div class="cart-icons">
                <ul>
                    <li>
                      <% if current_user != nil%>
                        <%= link_to "", user_path(current_user.id), class: "fa-lg far fa-user" %>
                      <% else %>
                        <%= link_to "", new_user_session_path, class: "fa-lg far fa-user" %>
                      <% end %>
                    </li>
                    <li></li>
                    <li>
                        <% if current_user != nil && current_user.cart != nil%>
                          <%= link_to "", user_cart_path(current_user.id, current_user.cart.id), class: "fa-lg fas fa-cart-arrow-down" %>
                        <% else %>
                          <%= link_to "", new_user_session_path, class: "fa-lg fas fa-cart-arrow-down" %>
                        <% end %>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
<nav class="navbar navbar-expand-lg navbar-light">
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse justify-content-center" id="navbarSupportedContent">
        <ul class="navbar-nav ">
            <li class="nav-item active">
                <%= link_to "home", root_path, class: "nav-link" %>
            </li>
